#proc assert(cond: Bool)

proc main():int = {
  {
    let x:int = 1
    let y:int = 2
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x:float = 1.0
    let y:float = 2.0
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x: float = 1.0
    let y: float = 2.0
    let z: float = 3.0
    printf("abc %f\n", (x + y) * z)
    printf("abc %f\n", (x + y) * z)
  }
  0
}

#proc assert(cond: Bool)

proc main():int32 = {
  {
    let x = int32(1)
    let y = int32(2)
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x = float32(1.0)
    let y = float32(2.0)
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x = float32(1.0)
    let y = float32(2.0)
    let z = float32(3.0)
    printf("abc %f\n", (x + y) * z)
  }
  int32(0)
}

#proc assert(cond: Bool)

proc main():i32 = {
  {
    let x = 1:i32
    let y = 2:i32
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x = 1.0:f32
    let y = 2.0:f32
    assert((x <  y) == true)
    assert((x <= y) == true)
    assert((x == y) == false)
    assert((x >= y) == false)
    assert((x >  y) == false)
  }

  {
    let x = 1.0:f64
    let y = 2.0:f64
    let z = 3.0:f64
    printf("abc %f\n", (x + y) * z)
  }
  0
}
